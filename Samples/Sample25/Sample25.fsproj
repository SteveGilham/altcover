  <ItemGroup>
    <ProjectReference Include="..\..\AltCover.Recorder\AltCover.Recorder.fsproj" />
    <Reference Include="FSharp.Core">
      <HintPath>..\..\packages\fsharp.core\4.1.18\lib\net20\FSharp.Core.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>